{
  "name": "snappet",
  "version": "0.0.1",
  "description": "Gist manager with tag support",
  "repository": {
    "type": "git",
    "url": "github.com/nonlogs"
  },
  "main": "index.js",
  "engines": {
    "node": ">=6.11.5"
  },
  "scripts": {
    "server": "./node_modules/.bin/nodemon --exec ./node_modules/.bin/babel-node --inspect ./server/index.js --env dev",
    "client": "webpack-dev-server --https --mode development --devtool inline-source-map --hot",
    "watch": "concurrently \"yarn server\" \"yarn client\"",
    "build:server": "./node_modules/.bin/webpack --config webpack-server.config.js --env prod",
    "build:client": "./node_modules/.bin/webpack --config webpack-client.config.js --env prod",
    "build:prod": "yarn clean:built-files && ./node_modules/.bin/webpack --config webpack.config.js --env prod",
    "bundle-report": "webpack --profile --json > dist/stats.json && webpack-bundle-analyzer --port 4200 dist/stats.json",
    "clean": "rm -rf dist/* && yarn run prepcerts",
    "clean:built-files": "find dist/* ! -name 'key.pem' ! -name 'cert.pem' -type f -exec rm -f {} +",
    "test": "jest",
    "prepvapid": "web-push generate-vapid-keys --json > ./private/vapid.json",
    "prepcerts": "yarn run prepvapid && ./node_modules/.bin/babel-node ./server/gen-certs.js && rm -rf dist/*.pem && mkdir -p dist && cp private/*.pem dist/",
    "sementic-release": "sementic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "author": "Connie Yu",
  "license": "ISC",
  "dependencies": {
    "apollo-server-restify": "^1.3.6",
    "axios": "^0.18.0",
    "bunyan": "^1.8.12",
    "chalk": "^2.4.1",
    "client-sessions": "^0.8.0",
    "concurrently": "^4.1.0",
    "cookie-session": "^2.0.0-beta.3",
    "cookies": "^0.7.3",
    "debug": "^4.1.1",
    "devcert-with-localhost": "^0.3.3",
    "dotenv": "^6.2.0",
    "graphql": "^14.0.2",
    "idb": "^2.1.3",
    "mongoose": "^5.4.3",
    "mongoose-timestamp": "^0.6.0",
    "passport": "^0.4.0",
    "passport-github2": "^0.1.11",
    "passport-restify": "^1.0.1",
    "restify": "^7.5.0",
    "restify-cookies": "^0.2.5",
    "restify-cors-middleware": "^1.1.1",
    "restify-mongoose": "^4.0.0",
    "restify-router": "^0.5.1",
    "web-push": "^3.3.3"
  },
  "devDependencies": {
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@types/react": "16.7.18",
    "@types/react-dom": "16.0.11",
    "@types/webpack-hot-middleware": "^2.16.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.0",
    "dotenv-webpack": "^1.6.0",
    "eslint": "^5.11.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.3",
    "react-test-renderer": "^16.7.0",
    "regenerator-runtime": "^0.13.1",
    "sass-loader": "^7.1.0",
    "semantic-release": "^15.13.2",
    "style-loader": "^0.23.1",
    "travis-deploy-once": "^5.0.11",
    "webpack": "^4.28.3",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-manifest-plugin": "^2.0.4",
    "worker-loader": "^2.0.0"
  }
}
